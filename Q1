import math

def calculate_distance(movements):
    # Initialize position
    x, y = 0, 0
    # Position
    for direction, distance in movements:
        if direction == 'UP':
            y += distance
        elif direction == 'DOWN':
            y -= distance
        elif direction == 'RIGHT':
            x += distance
        elif direction == 'LEFT':
            x -= distance
    # Calculate distance
    distance = math.sqrt(x**2 + y**2)
    # Print result
    print(round(distance))

#Defined movements
robot_movements = [('UP', 1), ('DOWN', 5), ('RIGHT', 12), ('LEFT', 9)]
# Calculate distance
calculate_distance(robot_movements)
